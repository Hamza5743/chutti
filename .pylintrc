[PYLINT]
max-line-length = 88  # black compatibility, do not remove
disable =
    arguments-differ,
    broad-except,
    duplicate-code,
    fixme,
    global-statement,
    import-outside-toplevel,
    invalid-name,
    line-too-long,
    missing-class-docstring,
    missing-function-docstring,
    missing-module-docstring,
    protected-access,
    singleton-comparison,
    stop-iteration-return,
    too-few-public-methods,
    too-many-ancestors,
    too-many-arguments,
    too-many-boolean-expressions,
    too-many-branches,
    too-many-instance-attributes,
    too-many-lines,
    too-many-locals,
    too-many-nested-blocks,
    too-many-public-methods,
    too-many-return-statements,
    too-many-statements,
    unused-argument,
    unnecessary-comprehension,
    unused-variable,
    using-constant-test,

